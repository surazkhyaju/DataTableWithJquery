@model DataTableCRUD.Models.Module

@using (Html.BeginForm("addData", "Module", FormMethod.Post, new { @class = "form-horizontal" ,id="postDataForm"}))
{
  
        <div class="form-group">
            @Html.LabelFor(cd => cd.ModuleName, new { @class = "col-md-3 control-label" })
            <div class="col-md-9">
                @Html.TextBoxFor(cd => cd.ModuleName, new { @class = "form-control" })
                @Html.ValidationMessageFor(cd => cd.ModuleName)
            </div>
        </div>
        <div class="form-actions">
            <div class="row">
                <div class="col-md-offset-3 col-md-9">
                    <button type="button" id="saveModule" class="btn btn-primary">Submit</button>
                    <button type="button" class="btn btn-default">Cancel</button>
                </div>
            </div>

        </div>

}


